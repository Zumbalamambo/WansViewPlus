<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@drawable/shape_signin_more"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_qualities"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_three"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_height"
            android:gravity="center"
            android:padding="10dp"
            android:text="@string/device_setting_quality_fhd"
            android:textColor="@color/gray_first"
            android:textSize="@dimen/signIn_textSize_16"
            android:visibility="gone"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_two"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_height"
            android:gravity="center"
            android:padding="10dp"
            android:text="@string/device_setting_quality_hd"
            android:textColor="@color/gray_first"
            android:textSize="@dimen/signIn_textSize_16"
            android:visibility="gone"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/tv_one"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_height"
            android:gravity="center"
            android:padding="10dp"
            android:text="@string/device_setting_quality_hd"
            android:textColor="@color/gray_first"
            android:textSize="@dimen/signIn_textSize_16"
            android:visibility="gone"
            tools:visibility="visible" />
    </LinearLayout>


</LinearLayout>